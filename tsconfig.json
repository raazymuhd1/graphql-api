{
  "compilerOptions": {
    "target": "ES2022",                                  
    "module": "NodeNext",                                
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,                                  
    "skipLibCheck": true,
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
    "outDir": "./dist",
    "rootDir": "./src"
  }
}
